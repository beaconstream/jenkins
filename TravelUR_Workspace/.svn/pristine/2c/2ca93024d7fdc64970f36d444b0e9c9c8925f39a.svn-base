package com.travelur.travelconnect.vacationpackages.models;

/**
 * @author by Abhijit.
 */

public class VacationPackageMoreDetailsCancellationPolicyListItem {

    String cancellation_penality;
    String cancellation_advance;

    public VacationPackageMoreDetailsCancellationPolicyListItem(String cancellation_advance, String cancellation_penality) {
        this.cancellation_advance = cancellation_advance;
        this.cancellation_penality = cancellation_penality;
    }

    public String getCancellation_advance() {
        return cancellation_advance;
    }

    public void setCancellation_advance(String cancellation_advance) {
        this.cancellation_advance = cancellation_advance;
    }

    public String getCancellation_penality() {
        return cancellation_penality;
    }

    public void setCancellation_penality(String cancellation_penality) {
        this.cancellation_penality = cancellation_penality;
    }


}
